{
    "name": "pancini/simplepermission",
    "minimum-stability": "stable",
    "description": "Simple package to test packagist.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Pancini",
            "email": "rpancini@gmail.com",
            "homepage": "https://github.com/ricpan/SimplePermission"
        }
    ],
    "homepage": "https://github.com/ricpan/SimplePermission",
    "keywords": ["Laravel", "SimplePermission"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pancini\\SimplePermission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pancini\\SimplePermission\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pancini\\SimplePermission\\SimplePermissionServiceProvider"
            ],
            "aliases": {
                "SimplePermission": "Pancini\\SimplePermission\\Facades\\SimplePermission"
            }
        }
    }
}
